digraph G {
  rankdir=LR;
  q0 [shape=doublecircle, margin=0];
  start0 [shape=none, style=invis];
  start0 -> q0 [label="START"];
  q0 -> q0  [label="\"x,p\""];
  invalid -> invalid  [label="\"*\""];
  q0 -> invalid  [label="\"q\""];
  invalid [shape=circle, margin=0];
}
