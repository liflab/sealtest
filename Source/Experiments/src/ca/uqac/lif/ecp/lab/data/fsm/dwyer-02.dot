# -----------------------------------
# Title: P is false after Q
# From:  http://patterns.projects.cs.ksu.edu/documentation/patterns/ltl.shtml
# -----------------------------------

digraph G {
  0 -> 1 [label="Q"];
  0 -> 0 [label="P,R,S"];
  1 -> 1 [label="Q,R,S"];
  1 -> 2 [label="P"];
  2 -> 2 [label="P,Q,R,S"];
}