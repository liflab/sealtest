# -----------------------------------
# Title: P is false between Q and R
# From:  http://patterns.projects.cs.ksu.edu/documentation/patterns/ltl.shtml
# -----------------------------------


digraph G {
  0 -> 1 [label="Q"];
  0 -> 0 [label="P,R,S"];
  1 -> 1 [label="Q,S"];
  1 -> 2 [label="P"];
  1 -> 0 [label="R"];
  2 -> 2 [label="P,Q,S"];
  2 -> 3 [label="R"];
  3 -> 3 [label="P,Q,R,S"];
}