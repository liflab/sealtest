digraph G {
  rankdir=LR;
  q0 [shape=doublecircle, margin=0];
  q1 [shape=doublecircle, margin=0];
  start0 [shape=none, style=invis];
  q0 -> q0  [label="\"s,x\""];
  q1 -> q0  [label="\"s,p\""];
  q0 -> q1  [label="\"r,t,p\""];
  q1 -> q1  [label="\"t,r,x\""];
  start0 -> q0 [label="START"];
}
